{
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true
  },
  "rules": {
    // "linebreak-style": ["error", "windows"],
    "linebreak-style": ["error", "unix"],
    "prefer-stateless-function": [0],
    "import/no-extraneous-dependencies": [0],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
